a.back,
button.back {
  font-weight: bold;
  display: inline-flex;
  align-items: center;
  border: none !important;
  &.red {
    color: #EC776A !important;
  }
  &.dark-blue {
    color: #32536B !important;
  }
  padding: 0 !important;
  &:before {
    content: '\21E6';
    font-size: 2em;
    font-weight: bold;
  }
  span {
    margin-left: 4px;
  }
}

button,
a {
  cursor: pointer;
  outline: none;
  text-decoration: none;
  &.outline {
    position: relative;
    background: transparent;
    color: #1172c4;
    font-size: 14px;
    border-color: #1172c4;
    border-style: solid;
    border-width: 1px;
    border-radius: 22px;
    padding: 10px 40px;
    transition: all 0.2s linear;
    a {
      text-decoration: none;
    }
    &:hover {
      color: white;
      background: #1172c4;
      border-color: white;
      transition: all 0.2s linear;
    }
    &:active {
      border-radius: 22px;
    }
    &.red {
